{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "moduleResolution": "node",
    "rootDir": "electron",
    "outDir": "build",
    "lib": ["es2021", "ES2021.String"],
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules"],
  "include": ["electron/*"]
}
