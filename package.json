{
  "name": "wow-combat-parser",
  "version": "0.0.0",
  "main": "packages/obscure-electron/dist/main.js",
  "description": "A simple wow combat log parser",
  "author": {
    "name": "Mackenzie Greco",
    "email": "mac.frozencoffee@gmail.com"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "build:dbmine": "yarn workspace @obscure/data-mine build",
    "start:dbmine": "yarn workspace @obscure/data-mine start",
    "start:ui": "yarn workspace @obscure/ui start",
    "build:ui": "yarn workspace @obscure/ui build",
    "start:electron": "yarn workspace @obscure/electron start",
    "debug:electron": "yarn workspace @obscure/electron debug",
    "build:electron": "yarn workspace @obscure/electron build",
    "build:types": "yarn workspace @obscure/types build",
    "build": "yarn build:types && yarn build:ui && yarn build:electron && yarn package",
    "package": "electron-builder build --config ./electron-builder.config.js",
    "lint": "yarn lint:vite && yarn lint:electron",
    "lint:ui": "yarn workspace @obscure/ui lint",
    "lint:electron": "yarn workspace @obscure/electron lint",
    "test:ui": "yarn workspace @obscure/ui test",
    "test:electron": "yarn workspace @obscure/electron test",
    "storybook": "yarn workspace @obscure/ui storybook"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.125",
    "@mui/material": "^5.11.16",
    "@mui/x-data-grid": "^6.0.4",
    "@tanstack/react-query": "^5.7.2",
    "axios": "^1.6.0",
    "clsx": "^1.2.1",
    "comlink": "^4.4.1",
    "csv": "^6.3.5",
    "dotenv": "^16.3.1",
    "electron-squirrel-startup": "^1.0.0",
    "i18next": "^22.4.15",
    "knex": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-router-dom": "^6.10.0",
    "sqlite3": "^5.1.6",
    "subscribableevent": "^1.0.1",
    "uuid": "^9.0.0",
    "vue-tsc": "^1.8.22"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@faker-js/faker": "^8.2.0",
    "@jest/globals": "^29.5.0",
    "@tanstack/eslint-plugin-query": "^5.6.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-jest": "^29.5.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^27.1.2",
    "electron-builder": "^23.6.0",
    "electron-rebuild": "^3.2.9",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^21.1.1",
    "node-loader": "^2.0.0",
    "prettier": "2.8.7",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-comlink": "^3.0.5",
    "vite-plugin-eslint": "^1.8.1"
  }
